# Podfile
use_frameworks!

target 'RxSwiftCrash' do
    pod 'RxSwift', '~> 5'
    pod 'RxCocoa', '~> 5'
end

# RxTest and RxBlocking make the most sense in the context of unit/integration tests
target 'RxSwiftCrashTests' do
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
    pod 'RxCocoa', '~> 5'
end
